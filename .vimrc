set history=1000
set undolevels=100
set ruler
set autoread
set cursorline
set number
set numberwidth=4
set rnu
set nobomb
set clipboard+=unnamed
set splitright
set splitbelow
set scrolloff=5
set showtabline=2
set synmaxcol=128
set viminfo=
filetype on
filetype indent on
filetype plugin on
syntax on
set hlsearch
set incsearch
set ignorecase
set nobackup
set noswapfile
set nowritebackup
set expandtab
set softtabstop=2
set shiftwidth=2
set tabstop=2
set visualbell
set noerrorbells
set t_vb=
set tm=500
set encoding=utf-8
scriptencoding utf-8
set wildignore+=*.o,*.obj,*.pyc
set wildignore+=*.png,*.jpg,*.gif,*.ico
set wildignore+=*.swf,*.fla
set wildignore+=*.mp3,*.mp4,*.avi,*.mkv

nmap <TAB> v>
nmap <S-TAB> v<
vmap <TAB> >gv
vmap <S-TAB> <gv

map 0 ^

